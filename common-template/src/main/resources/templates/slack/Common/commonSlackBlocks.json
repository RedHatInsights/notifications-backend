{
    "blocks": [
        {
            "type": "header",
            "text": {
                "type": "plain_text",
                "text": "{data.source.event_type.display_name} - {data.source.application.display_name} - {data.source.bundle.display_name}",
                "emoji": true
            }
        },
        {#if data.severity??}
        {
            "type": "context",
            "elements": [
                {
                    "type": "mrkdwn",
                    "text": "{data.severity.asSeverityEmoji} {#insert severity-label}Severity:{/} {data.severity.toTitleCase}"
                }
            ]
        },
        {/if}
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "{#insert content-intro}{#if data.context.display_name??}<{data.inventory_url.raw}|{data.context.display_name}> triggered {data.events.size()} event{#if data.events.size() != 1}s{/if}{#else}{data.events.size()} event{#if data.events.size() != 1}s{/if} triggered{/if}.{/}"
            }
        },
        {
            "type": "divider"
        },
        {#insert content-body}{/}
        {
            "type": "section",
            "text": {
                "type": "mrkdwn",
                "text": "Explore {#if data.events.size() != 1}these{#else}this{/if} and others in *<{#insert explore-url}{data.application_url.raw}{/}|{#insert explore-location}{data.source.application.display_name}{/}>*."
            }
        }
    ]
}